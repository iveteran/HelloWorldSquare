<clientConfig version="1.1">
  <emailProvider id="goe.works">
    <domain>goe.works</domain>
    <displayName>GOE Works</displayName>
    <displayShortName>GOE</displayShortName>
    <incomingServer type="imap">
      <hostname>mail.goe.works</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-encrypted</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>mail.goe.works</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILLOCALPART%</username>
      <authentication>password-encrypted</authentication>
    </outgoingServer>
    <documentation url="https://goe.works/supports"/>
  </emailProvider>
</clientConfig>
